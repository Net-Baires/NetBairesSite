@model List<Models.SponsorViewModel>
<h3>Sponsors</h3>
<div class="card-group">
    @foreach (var sponsor in Model)
    {

        <div class="card" style="width: 18rem; float: left">
            <a asp-page="./Sponsors/Detail" asp-route-id="@sponsor.Name">
                <img class="card-img-top" src="@sponsor.ImageUrl" alt="Card image cap">
            </a>
            <div class="card-body">
                <h5 class="card-title">@sponsor.Name</h5>
                <p class="card-text">@sponsor.Info</p>
            </div>
        </div>
    }
</div>
